{
    "openapi": "3.0.1",
    "info": {
        "title": "International Shipping",
        "description": "The International Shipping API enables sellers to use the Ship with Walmart (SWW) International shipping programs to ship their goods from the US to Canada. These APIs provide support for two SWW Programs : SWW Express & SWW Ground",
        "version": "0.7.0"
    },
    "servers": [
        {
            "url": "https:\/\/marketplace.walmartapis.com",
            "description": "Production URL"
        }
    ],
    "paths": {
        "\/v3\/ca\/shipping\/labels": {
            "post": {
                "tags": [
                    "International Shipping"
                ],
                "summary": "Create label",
                "description": "Create shipping label for items. The response to a successful call are of json or pdf type based on the media type passed in Accept header.\nFor eg. 'Accept'='application\/json,application\/pdf' will result in pdf media type and\n'Accept'='application\/json' as json response.",
                "operationId": "createLabel",
                "parameters": [
                    {
                        "name": "WM.TEST_MODE",
                        "in": "header",
                        "description": "For sellers\/clients who are in the process of on boarding or already on boarded to IMD platforms, this feature allows them to test the API integration to SWW international label generation API. Sellers get a response which maps their request attributes with some additional static information like tracking and label. The label returned is corresponding to the carrier configured for each seller, in case if the configuration is still in progress a sample Fedex Express label is returned.",
                        "required": false,
                        "schema": {
                            "type": "boolean",
                            "default": false
                        }
                    }
                ],
                "requestBody": {
                    "description": "Label fields",
                    "content": {
                        "application\/json": {
                            "schema": {
                                "$ref": "#\/components\/schemas\/LabelGenerationRequestCa"
                            },
                            "example": {
                                "packageType": "CUSTOM_PACKAGE",
                                "boxDimensions": {
                                    "boxDimensionUnit": "IN",
                                    "boxWeightUnit": "LB",
                                    "boxWeight": 1,
                                    "boxLength": 100,
                                    "boxWidth": 1,
                                    "boxHeight": 1
                                },
                                "boxItems": [
                                    {
                                        "sku": "SKU_28072021",
                                        "quantity": 1
                                    }
                                ],
                                "fromAddress": {
                                    "contactName": "Test",
                                    "companyName": "Walmart",
                                    "addressLine1": "Add1",
                                    "addressLine2": "Add2",
                                    "city": "Anchorage",
                                    "state": "AK",
                                    "postalCode": "99501",
                                    "country": "US",
                                    "phone": "12253",
                                    "email": "test@walmart.com"
                                },
                                "purchaseOrderId": "P100569013",
                                "carrierName": "FedEx",
                                "carrierServiceType": "FEDEX_INTERNATIONAL_ECONOMY",
                                "labelSize": "A6"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "Successful Operation",
                        "content": {
                            "application\/json": {
                                "schema": {
                                    "$ref": "#\/components\/schemas\/CommonResponseLabelGenerationResponseCa",
                                    "x-expose-headers": true
                                },
                                "example": {
                                    "data": {
                                        "id": "877E846379A00000C8C48F11",
                                        "purchaseOrderId": "P100569013",
                                        "trackingNo": "019154094082",
                                        "boxItems": [
                                            {
                                                "sku": "SKU_28072021",
                                                "quantity": 1
                                            }
                                        ],
                                        "carrierName": "FedEx",
                                        "carrierFullName": "FedEx",
                                        "carrierServiceType": "FedEx International Priority",
                                        "trackingUrl": "https:\/\/www.fedex.com\/apps\/fedextrack\/?action=track&tracknumbers=019154094082&locale=en_US&cntry_code=us",
                                        "shippingDocuments": [
                                            {
                                                "documentFormat": "PDF",
                                                "documentType": "LABEL",
                                                "base64EncodedDocument": ""
                                            }
                                        ]
                                    }
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "channelType": [],
                        "signature": [],
                        "consumerId": []
                    }
                ]
            }
        },
        "\/v3\/ca\/shipping\/labels\/shipment\/international\/consolidation": {
            "get": {
                "tags": [
                    "International Shipping"
                ],
                "summary": "Get consolidation details",
                "description": "Use this API to get consolidation details for the shipment. Provide the domestic Tracking Number and Carrier Id. The synchronous API call returns a list of packages consolidated under that tracking number in response. In case of any failure, the client has to retry.",
                "operationId": "getConsolidation",
                "parameters": [
                    {
                        "name": "domesticTrackingNo",
                        "in": "query",
                        "description": "Domestic TrackingNo.",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "domesticCarrierId",
                        "in": "query",
                        "description": "Domestic CarrierId.",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "format": "int32"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful Operation",
                        "content": {
                            "application\/json": {
                                "schema": {
                                    "$ref": "#\/components\/schemas\/CommonResponseConsolidationResponse",
                                    "x-expose-headers": true
                                },
                                "example": {
                                    "data": {
                                        "consolidationId": 1873,
                                        "shipmentIds": [
                                            "875F143972000000CA8020EA"
                                        ],
                                        "domesticTrackingNo": "1234",
                                        "domesticCarrierId": 1003
                                    }
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "channelType": [],
                        "signature": [],
                        "consumerId": []
                    }
                ]
            },
            "post": {
                "tags": [
                    "International Shipping"
                ],
                "summary": "Create consolidation",
                "description": "Use this API to consolidate shipments. Provide the list of packages that form part of the consolidation and its corresponding domestic tracking number and carrier. The synchronous API call returns the status of the operation and a correlation ID in response. Use the correlation ID to connect via email to the IMD team in case of issues pertaining to the consolidation call. In case of any failure, the client has to retry.",
                "operationId": "createConsolidation",
                "parameters": [],
                "requestBody": {
                    "description": "Consolidation Request",
                    "content": {
                        "application\/json": {
                            "schema": {
                                "$ref": "#\/components\/schemas\/ConsolidationRequest"
                            },
                            "example": {
                                "domesticCarrierId": 1000,
                                "domesticTrackingNo": "20128590000367",
                                "shipmentIds": [
                                    "875F143972000000CA8020EA"
                                ]
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "Successful Operation",
                        "content": {
                            "application\/json": {
                                "schema": {
                                    "$ref": "#\/components\/schemas\/CommonResponseConsolidationResponse",
                                    "x-expose-headers": true
                                },
                                "example": {
                                    "data": {
                                        "consolidationId": 1873,
                                        "shipmentIds": [
                                            "875F143972000000CA8020EA"
                                        ],
                                        "domesticTrackingNo": "1234",
                                        "domesticCarrierId": 1003
                                    }
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "channelType": [],
                        "signature": [],
                        "consumerId": []
                    }
                ]
            }
        },
        "\/v3\/ca\/shipping\/labels\/purchase-orders\/{purchaseOrderId}": {
