                                                "sku": "CNE498398",
                                                "availabilityCode": "AC",
                                                "quantity": {
                                                    "unit": "EACH",
                                                    "amount": "10"
                                                }
                                            },
                                            {
                                                "sku": "ard65005",
                                                "availabilityCode": "AC",
                                                "quantity": {
                                                    "unit": "EACH",
                                                    "availableToSell": "5",
                                                    "orderInProgress": "3"
                                                }
                                            }
                                        ]
                                    }
                                },
                                "DSV_MultiNode_Bulk_Inventory_Update_Request": {
                                    "value": {
                                        "inventoryHeader": {
                                            "version": "1.5"
                                        },
                                        "inventory": [
                                            {
                                                "sku": "190086893939",
                                                "shipNodes": [
                                                    {
                                                        "shipNode": "10000003527",
                                                        "availabilityCode": "AC",
                                                        "quantity": {
                                                            "unit": "EACH",
                                                            "amount": 100
                                                        }
                                                    }
                                                ]
                                            },
                                            {
                                                "sku": "685387364107_SS4_01",
                                                "shipNodes": [
                                                    {
                                                        "shipNode": "10000003527",
                                                        "availabilityCode": "AC",
                                                        "quantity": {
                                                            "unit": "EACH",
                                                            "amount": 21
                                                        }
                                                    }
                                                ]
                                            },
                                            {
                                                "sku": "5700521553133_toolsnonwfs_2",
                                                "shipNodes": [
                                                    {
                                                        "shipNode": "10000003527",
                                                        "availabilityCode": "AC",
                                                        "quantity": {
                                                            "unit": "EACH",
                                                            "amount": 13
                                                        }
                                                    },
                                                    {
                                                        "shipNode": "1000000352",
                                                        "availabilityCode": "AC",
                                                        "quantity": {
                                                            "unit": "EACH",
                                                            "amount": 12
                                                        }
                                                    }
                                                ]
                                            },
                                            {
                                                "sku": "15088243",
                                                "shipNodes": [
                                                    {
                                                        "shipNode": "10000003527",
                                                        "availabilityCode": "AC",
                                                        "quantity": {
                                                            "unit": "EACH",
                                                            "amount": 24
                                                        }
                                                    },
                                                    {
                                                        "shipNode": "1000052",
                                                        "availabilityCode": "AC",
                                                        "quantity": {
                                                            "unit": "EACH",
                                                            "amount": 12
                                                        }
                                                    }
                                                ]
                                            },
                                            {
                                                "sku": "AD-01-white-5",
                                                "shipNodes": [
                                                    {
                                                        "shipNode": "10000003527",
                                                        "availabilityCode": "AC",
                                                        "quantity": {
                                                            "unit": "EACH",
                                                            "amount": 20
                                                        }
                                                    }
                                                ]
                                            }
                                        ]
                                    }
                                },
                                "2.X": {
                                    "value": {
                                        "InventoryFeedHeader": {
                                            "feedType": "DSV_INVENTORY",
                                            "version": "2.0.20220829-15_11_50-api",
                                            "businessUnit": "WALMART_US",
                                            "locale": "en",
                                            "feedDate": "2023-03-13T13:03:33.000Z"
                                        },
                                        "Inventory": [
                                            {
                                                "quantity": 10,
